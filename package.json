{
  "name": "@cocalc/fuse-native",
  "version": "2.4.1",
  "description": "Node.js bindings for the FUSE api (Filesystem in Userspace)",
  "main": "index.js",
  "bin": {
    "fuse-native": "./bin.js"
  },
  "scripts": {
    "install": "node-gyp-build",
    "test": "jest test/*.js",
    "test:fuse": "jest test/fuse-operations.test.js",
    "test:operations": "jest test/fuse-operations.test.js",
    "test:verbose": "jest test/*.js --verbose",
    "test:watch": "jest test/*.js --watch",
    "example:memory-fs": "node test/example-usage.js",
    "clean": "rm -rf node_modules build",
    "prebuild": "prebuildify --napi --strip",
    "prebuild-ia32": "prebuildify --napi --strip --arch=ia32",
    "configure": "NODE=$(which node) && sudo -E $NODE ./bin.js configure || true"
  },
  "gypfile": true,
  "dependencies": {
    "nanoresource": "^1.3.0",
    "napi-macros": "^2.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "jest": "^30.1.3",
    "prebuildify": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/fuse-native.git"
  },
  "author": " ()",
  "contributors": [
    {
      "name": "Mathias Buus",
      "email": "@mafintosh"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sagemathinc/fuse-native/issues"
  },
  "homepage": "https://github.com/sagemathinc/fuse-native"
}
